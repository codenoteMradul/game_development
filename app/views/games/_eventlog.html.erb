<table class="eventlog_table">
 <tr>
  <th>Time</th>
  <th>Username</th>
  <th>Actions </th>
 </tr>
 <tr>
   <% @events.each do |event| %>
   <td><%= event.time %></td>
   <td><%= event.username %></td>
   <td><%= event.actions %></td>
 </tr>
   <% end %>
</table>
<br>
<span class="pagination"><%= will_paginate @event %></span> 
<br>
